{
  "name": "bitcoin-investments",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "deploy": "npm run build && npx wrangler pages deploy dist",
    "dev:wrangler": "npm run build && npx wrangler pages dev dist",
    "cf:deploy": "npx wrangler pages deploy dist --project-name=bitcoin-investments",
    "cf:tail": "npx wrangler pages deployment tail",
    "deploy:cron": "npx wrangler deploy --config wrangler-cron.toml",
    "cron:tail": "npx wrangler tail price-alerts-cron",
    "cron:test": "curl -X POST https://bitcoinvestments.net/api/check-price-alerts -H \"Authorization: Bearer $SUPABASE_SERVICE_ROLE_KEY\"",
    "deploy:all": "npm run build && npm run cf:deploy && npm run deploy:cron"
  },
  "dependencies": {
    "@gsap/react": "^2.1.2",
    "@rainbow-me/rainbowkit": "^2.1.6",
    "@react-three/drei": "^9.92.7",
